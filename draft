# a
def sum(wea,supply):
    dr = wea * supply
    return dr

calculation = sum(70000000, 2)
print(calculation)

# b
def sum(a,b):
    gaslo = a + b
    return gaslo

say_it = sum('Slava', ' Ukraini!')
print(say_it)

# c
def tuple(*args):
    return args

result = tuple('Heroyam', ' Slava!')
print(result)
